package com.example.tabhost;

public class FlimStarInformationModal {
	public String FilmStarName;
	public int FlimStarImageID;
	public String FilmStarBio1;
	public String FilmStarBio2;
	
	public void setFilmStarName(String name){
		FilmStarName=name;
		
	}
	public void setFilmStarImageId(int id){
		FlimStarImageID=id;
		
	}
	public void setFilmStarBio1(String bio1){
		FilmStarBio1=bio1;
		
	}
	public void setFilmStarBio2(String bio2){
		FilmStarBio2=bio2;
		
	}
}
